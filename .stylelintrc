{
	"extends": [
		"stylelint-config-standard",
		"stylelint-order-config-standard"
	],
	"plugins": [
		"stylelint-order"
	],
	"rules": {
		"indentation": [
			2
		],
		"color-hex-case": "upper",
		"selector-class-pattern": null,
		"declaration-block-no-redundant-longhand-properties": null,
		"alpha-value-notation": "percentage"
	}
}